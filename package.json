{
  "name": "ttg-club",
  "private": true,
  "type": "module",
  "scripts": {
    "serve": "vite --host",
    "build:dev": "vite build --emptyOutDir --mode development",
    "build:prod": "vite build --emptyOutDir --mode production",
    "type-check": "vue-tsc --noEmit -p tsconfig.json --composite false",
    "lint:check": "eslint --ignore-path .gitignore \"{**/*,*}.{cjs,js,ts,jsx,tsx,vue}\"",
    "lint:write": "eslint --fix --ignore-path .gitignore \"{**/*,*}.{cjs,js,ts,jsx,tsx,vue}\"",
    "prettier:check": "prettier . \"{**/*,*}.{cjs,js,ts,jsx,tsx,html,vue}\" --ignore-path .gitignore --check",
    "prettier:write": "prettier . \"{**/*,*}.{cjs,js,ts,jsx,tsx,html,vue}\" --ignore-path .gitignore --write",
    "pre-commit": "run-s lint:write prettier:write",
    "prepare": "is-ci || husky install"
  },
  "dependencies": {
    "@svifty7/vue-paginate": "^0.0.10",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "@vueuse/core": "^10.4.1",
    "axios": "^1.5.0",
    "clsx": "^2.0.0",
    "dayjs": "^1.11.9",
    "dice-roller-parser": "^0.1.8",
    "js-cookie": "^3.0.5",
    "js-file-downloader": "^1.1.25",
    "localforage": "^1.10.0",
    "lodash-es": "^4.17.21",
    "normalize.css": "^8.0.1",
    "pinia": "^2.1.6",
    "tippy.js": "^6.3.7",
    "ua-parser-js": "^1.0.35",
    "uuid": "^9.0.0",
    "vue": "^3.3.4",
    "vue-easy-lightbox": "^1.16.0",
    "vue-final-modal": "^4.4.5",
    "vue-gtag": "^2.0.1",
    "vue-lazyload": "^3.0.0",
    "vue-multiselect": "^3.0.0-beta.2",
    "vue-router": "^4.2.4",
    "vue-tippy": "^6.3.1",
    "vue-toastification": "^2.0.0-rc.5",
    "vue-virtual-scroller": "^2.0.0-beta.8",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@types/js-cookie": "^3.0.3",
    "@types/lodash-es": "^4.17.9",
    "@types/ua-parser-js": "^0.7.37",
    "@types/uuid": "^9.0.3",
    "@types/vuelidate": "^0.7.18",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "@vitejs/plugin-legacy": "^4.1.1",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vue/eslint-config-airbnb": "^7.0.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.48.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-boundaries": "^3.4.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-vue": "^9.17.0",
    "eslint-plugin-vuejs-accessibility": "^2.2.0",
    "husky": "^8.0.3",
    "install": "^0.13.0",
    "is-ci": "^3.0.1",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.0.3",
    "sass": "^1.66.1",
    "terser": "^5.19.3",
    "typescript": "^5.2.2",
    "typescript-plugin-css-modules": "^5.0.1",
    "vite": "^4.4.9",
    "vite-plugin-ejs": "^1.6.4",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-eslint-parser": "^9.3.1",
