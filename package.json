{
  "name": "ttg-club",
  "private": true,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/TTG-Club/ttg-club-frontend.git"
  },
  "engines": {
    "node": ">=20.15.x",
    "npm": ">=10.5.x"
  },
  "engineStrict": true,
  "scripts": {
    "serve": "vite --host",
    "build:dev": "vite build --emptyOutDir --mode development",
    "build:prod": "vite build --emptyOutDir --mode production",
    "preview": "vite preview",
    "type-check": "vue-tsc -p tsconfig.json",
    "lint:check": "eslint \"{**/*,*}.{cjs,js,ts,jsx,tsx,vue}\" --cache --cache-strategy content --ignore-path .eslintignore",
    "lint:write": "eslint \"{**/*,*}.{cjs,js,ts,jsx,tsx,vue}\" --cache --cache-strategy content --ignore-path .eslintignore --fix",
    "lint:workflow": "eslint \"{**/*,*}.{cjs,js,ts,jsx,tsx,vue}\" --cache --cache-strategy content --ignore-path .eslintignore --quiet",
    "stylelint:check": "stylelint \"{**/*,*}.{css,scss,vue}\" --cache --cache-strategy content --ignore-path .eslintignore",
    "stylelint:write": "stylelint \"{**/*,*}.{css,scss,vue}\" --cache --cache-strategy content --ignore-path .eslintignore --fix",
    "stylelint:workflow": "stylelint \"{**/*,*}.{css,scss,vue}\" --cache --cache-strategy content --ignore-path .eslintignore --quiet",
    "pre-commit": "lint-staged",
    "prepare": "is-ci || husky"
  },
  "dependencies": {
    "@vueuse/core": "^10.11.0",
    "@vueuse/gesture": "^2.0.0",
    "@vueuse/math": "^10.11.0",
    "@vueuse/shared": "^10.11.0",
    "axios": "^1.7.3",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.12",
    "dice-roller-parser": "^0.1.8",
    "js-cookie": "^3.0.5",
    "js-file-downloader": "^1.1.25",
    "localforage": "^1.10.0",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.1",
    "naive-ui": "^2.39.0",
    "normalize.css": "^8.0.1",
    "pinia": "^2.2.0",
    "tippy.js": "^6.3.7",
    "ua-parser-js": "^1.0.38",
    "uuid": "^10.0.0",
    "vue": "^3.4.35",
    "vue-easy-lightbox": "^1.19.0",
    "vue-gtag": "^2.0.1",
    "vue-lazyload": "^3.0.0",
    "vue-multiselect": "^3.0.0",
    "vue-router": "^4.4.2",
    "vue-tippy": "^6.4.4",
    "vue-toastification": "^2.0.0-rc.5",
    "vue-virtual-scroller": "^2.0.0-beta.8",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.4",
    "@types/js-cookie": "^3.0.6",
    "@types/lodash-es": "^4.17.12",
    "@types/ua-parser-js": "^0.7.39",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-legacy": "^5.4.1",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vue/eslint-config-airbnb": "^8.0.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "<9",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-boundaries": "^4.2.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-vue": "^9.27.0",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "husky": "^9.1.4",
    "is-ci": "^3.0.1",
    "lint-staged": "^15.2.7",
    "postcss-html": "^1.7.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.3.3",
    "sass": "^1.77.8",
    "stylelint": "^16.8.1",
    "stylelint-config-clean-order": "^6.1.0",
    "stylelint-order": "^6.0.4",
    "stylelint-scss": "^6.4.1",
    "typescript": "^5.5.4",
    "typescript-plugin-css-modules": "^5.1.0",
    "unplugin-auto-import": "^0.18.2",
    "unplugin-vue-components": "^0.27.3",
    "vite": "^5.3.5",
    "vite-plugin-checker": "^0.7.2",
    "vite-plugin-ejs": "^1.7.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.0.29"
  }
}
